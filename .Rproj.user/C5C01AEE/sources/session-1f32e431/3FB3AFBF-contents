# Do this by running an IS with this llhd.

llhd_results = data.frame()

num_particles_vec = c(100,200,300)
epsilon_vec = c(10,1,0.1,0.01)
#epsilon_vec = c(10,1)

experiment = "lv_XY"
#experiment = "lv_XY_bad"
#experiment = "lv_X"
base_method = "pf"

model_pf = compile(paste("is_",base_method,"_",experiment,".ilike",sep=""))
