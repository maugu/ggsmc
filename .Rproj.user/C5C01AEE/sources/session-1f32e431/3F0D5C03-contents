# Plot bias, sd, RMSEscaled, RMSE v sig (line for ABC/SL)

# Plot RMSE against number of points with different line for each conditions.

epsilon = 10
eps_stats = all_statistics %>% dplyr::filter(ε==epsilon)
which(eps_stats$SD==min(eps_stats$SD,na.rm=TRUE))
which(eps_stats$RMSE==min(eps_stats$RMSE,na.rm=TRUE))
which(eps_stats$`Scaled MSE`==min(eps_stats$`Scaled MSE`,na.rm=TRUE))
# PF with 300 points

epsilon = 1
eps_stats = all_statistics %>% dplyr::filter(ε==epsilon)
which(eps_stats$SD==min(eps_stats$SD,na.rm=TRUE))
which(eps_stats$RMSE==min(eps_stats$RMSE,na.rm=TRUE))
which(eps_stats$`Scaled MSE`==min(eps_stats$`Scaled MSE`,na.rm=TRUE))
# sEnKI with est 1, sig 1, 20 targets, 100 points
## Revised scaled is still the same, but best RMSE is with 100 targets

epsilon = 0.1
eps_stats = all_statistics %>% dplyr::filter(ε==epsilon)
which(eps_stats$SD==min(eps_stats$SD,na.rm=TRUE))
which(eps_stats$RMSE==min(eps_stats$RMSE,na.rm=TRUE))
which(eps_stats$`Scaled MSE`==min(eps_stats$`Scaled MSE`,na.rm=TRUE))
# sEnKI with 100 points, est 1, sig 1, 20 targets, 100 points
## Revised scaled is still the same, but best RMSE is with 100 targets est 3, sig 1, 50 targets

epsilon = 0.01
eps_stats = all_statistics %>% dplyr::filter(ε==epsilon)
which(eps_stats$SD==min(eps_stats$SD,na.rm=TRUE))
