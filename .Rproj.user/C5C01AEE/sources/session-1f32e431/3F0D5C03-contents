which(eps_stats$`Scaled MSE`==min(eps_stats$`Scaled MSE`,na.rm=TRUE))
# Best RMSE is sEnKI with est 3, sig 0.5, 20 targets, 200 points
## Revised scaled is still the same, but best RMSE is sEnKI with est 3, sig 1, 50 targets, 200 points

epsilon = 0.01
eps_stats = all_enki_statistics %>% dplyr::filter(ε==epsilon) %>% dplyr::filter(Estimator!=3)
which(eps_stats$RMSE==min(eps_stats$RMSE,na.rm=TRUE))
which(eps_stats$`Scaled MSE`==min(eps_stats$`Scaled MSE`,na.rm=TRUE))
# Back to 100, 1, 1, 20
## Revised scaled is est 3, sig 0.5, 50 targets, 100 points, RMSE is same with 300 points

epsilon = 10

all_statistics %>%
  dplyr::filter(ε==epsilon) %>%
  dplyr::filter((Method=="ABC") | (Method=="SL") | (Method=="PF") | (Method=="sEnKF") | (Method=="rEnKF") | (Method=="aEnKF")) %>%
  #dplyr::filter(n==300) %>%
  ggplot( aes(x=n, y=Bias, group=interaction(Method), color=Method)) +
  geom_line()

all_statistics %>%
  dplyr::filter(ε==epsilon) %>%
  dplyr::filter((Method=="ABC") | (Method=="SL") | (Method=="PF") | (Method=="sEnKF") | (Method=="rEnKF") | (Method=="aEnKF")) %>%
  #dplyr::filter(n==300) %>%
  ggplot( aes(x=n, y=SD, group=interaction(Method), color=Method)) +
  geom_line()

all_statistics %>%
  dplyr::filter(ε==epsilon) %>%
