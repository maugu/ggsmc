{
shifter_result[j] = "Stochastic"
}
if (length(grep("aEnKI",enki_statistics$Method[j]))>0)
{
shifter_result[j] = "Adjustment"
}
if (length(grep("rEnKI",enki_statistics$Method[j]))>0)
{
shifter_result[j] = "Square root"
}
}
